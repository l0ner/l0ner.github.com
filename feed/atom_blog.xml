<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>l0ner.github.com - blog</title>
 <link href="http://l0ner.github.com/atom_blog.xml" rel="self"/>
 <link href="http://l0ner.github.com/blog/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://l0ner.github.com/</id>
 <author>
   <name>Pawel "l0ner" Soltys</name>
   <email>sh4dou@gmail.com</email>
 </author>

 {% for post in site.categories.blog %}
 <entry>
   <title>"{{ post.title | xml_escape }}"</title>
   <link href="http://l0ner.github.com/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://l0ner.github.com/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape | truncatewords: 100 }}<br /><a href="http://l0ner.github.com/{{ post.url }}">Read whole story</a></content>
 </entry>
 {% endfor %}
</feed>